{
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "description": "Merge multiple windows together",
  "homepage_url": "https://github.com/jonathanKingston/merge-windows",
  "manifest_version": 2,
  "name": "Merge Windows",
  "version": "1.0.2",
  "icons": {
    "96": "merge.svg"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "commands": {
    "merge-all-windows": {
      "description": "Merge all windows into the current one."
    },
    "merge-previous-window": {
      "description": "Merge previously focused window into the current one."
    }
  }
}
